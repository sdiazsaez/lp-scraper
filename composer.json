{
    "name": "larangular/lp-scraper",
    "description": "scraper support for chilean automobile license plate",
    "license": "MIT",
    "authors": [
        {
            "name": "Simón Díaz",
            "email": "sdiaz@sdshost.ml"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/support": "*",
        "larangular/support": "^0.1.0|^0.2.0",
        "symfony/dom-crawler": "^4.2|^5.1|^6.0",
        "guzzlehttp/guzzle": "^6.3|^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5|^9.3|^10.0",
        "orchestra/testbench": "~3.0|^4.0|^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Larangular\\LPScraper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Larangular\\LPScraper\\Tests\\": "./tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Larangular\\LPScraper\\LPScraperServiceProvider"
            ]
        }
    }
}
